{
  "name": "LOGILINK",
  "private": true,
  "version": "0.6.16",
  "description": "Logilink App",
  "title": "LOGILINK PRO",
  "author": "Intercocina",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/hagmir7/logilink.git"
  },
  "homepage": "https://intercocina.com",
  "main": "src/electron/main.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:react dev:electron",
    "dev:react": "vite",
    "dev:electron": "cross-env NODE_ENV=development electron .",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "dist:mac": "npm run transpile:electron && npm run build && electron-builder --mac --arm64",
    "dist:win": "npm run build && electron-builder --win --x64",
    "dist:linux": "npm run transpile:electron && npm run build && electron-builder --linux --x64",
    "publish": "npm run build && electron-builder --win --x64 --publish always"
  },
  "dependencies": {
    "@ericblade/quagga2": "^1.8.4",
    "@fontsource/inter": "^5.2.5",
    "@tailwindcss/vite": "^4.1.4",
    "@yudiel/react-qr-scanner": "^2.3.1",
    "antd": "^5.24.9",
    "antd-style": "^3.7.1",
    "axios": "^1.9.0",
    "bwip-js": "^4.8.0",
    "dayjs": "^1.11.13",
    "electron-updater": "^6.6.2",
    "html5-qrcode": "^2.3.8",
    "lodash": "^4.17.21",
    "lucide-react": "^0.503.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^7.5.3"
  },
  "devDependencies": {
    "@electron/fuses": "^1.8.0",
    "@eslint/js": "^9.21.0",
    "@types/react": "^18.0.10",
    "@types/react-dom": "^18.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "cross-env": "^7.0.3",
    "electron": "^35.1.4",
    "electron-winstaller": "^5.4.0",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^4.1.11",
    "vite": "^6.2.0"
  },
  "build": {
    "appId": "com.intercocina.logilink",
    "productName": "LOGILINK",
    "publish": {
      "provider": "github",
      "repo": "logilink",
      "owner": "hagmir7"
    },
    "win": {
      "target": "nsis",
      "executableName": "LOGILINK",
      "icon": "build/inter.png"
    }
  }
}
